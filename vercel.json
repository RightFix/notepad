{
{
  "src": "build_files.sh",
  "use": "@vercel/static-build",
  "config": { "distDir": "static" }
}
  "version": 2,
  "builds": [
    {
      "src": "bluesea_mobile/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "package.json",  // Optional if no Node deps; create empty if needed
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bluesea_mobile/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "bluesea_mobile/wsgi.py"
    }
  ]
}
