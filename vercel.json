{
  "version": 2,
  "builds": [
    {
      "src": "bluesea_mobile/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "30mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "bluesea_mobile/wsgi.py" }
  ]
}